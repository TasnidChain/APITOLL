{
  "name": "@apitoll/seller-sdk",
  "version": "0.1.0-beta.6",
  "description": "x402 payment middleware for API and MCP tool monetization. Add per-request micropayments in 3 lines of code.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://apitoll.com",
  "bugs": "https://github.com/TasnidChain/APITOLL/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/TasnidChain/APITOLL",
    "directory": "packages/seller-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
